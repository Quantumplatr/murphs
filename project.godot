; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Murph's"
config/version="v0.0.5-alpha"
run/main_scene="res://scenes/terminal/terminal.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://assets/logo/logo.png"

[autoload]

SettingsManager="*res://autoloads/settings/settings_manager.tscn"
Constants="*res://autoloads/constants.tscn"
Encryption="*res://autoloads/encryption.gd"
Sections="*res://autoloads/sections.gd"
AppManager="*res://autoloads/app_manager.tscn"
TaskManager="*res://autoloads/task_manager.tscn"
AudioManager="*res://autoloads/audio/audio_manager.tscn"
GameManager="*res://autoloads/game_manager/game_manager.gd"

[display]

window/size/viewport_width=1080
window/size/viewport_height=810
window/stretch/mode="viewport"

[file_customization]

folder_colors={
"res://assets/": "pink",
"res://drive/": "purple",
"res://scenes/": "green"
}

[gui]

theme/custom="res://assets/ui/main_theme.tres"
theme/custom_font="res://assets/fonts/SpaceMono/SpaceMono-Regular.ttf"

[rendering]

textures/vram_compression/import_etc2_astc=true

[richer_text_label]

fonts={
"AnonymousPro": "res://assets/fonts/Anonymous Pro/AnonymousPro-Regular.ttf",
"AnonymousProItalic": "res://assets/fonts/Anonymous Pro/AnonymousPro-BoldItalic.ttf",
"SpaceMono": "res://assets/fonts/SpaceMono/SpaceMono-Regular.ttf",
"SpaceMonoItalic": "res://assets/fonts/SpaceMono/SpaceMono-BoldItalic.ttf"
}
